@{
        var db = Database.Open("League_DB");
        var postLeagueid = Request["addleagueid"];
        var postTeamid = Request["addteamid"];
        var postTeam = Request["addteam"];
        var postName = Request["addplayer"];
        var postPosition = Request["addposition"];
        var postEmail = Request["addemail"];
        var postPhone = Request["addphone"];
        
        var postteamSQL = "INSERT INTO Team_Profile_tbl (League_ID, Team_UID, Team_Name, Name, Position, Email, Phone_Number) VALUES (@0, @1, @2, @3, @4, @5, @6)";
        db.Execute(postteamSQL, postLeagueid, postTeamid, postTeam, postName, postPosition, postEmail, postPhone); 

        var getTeamprofile = "Select Team_Profile_tbl.Team_Profile_ID from Team_Profile_tbl where League_ID = @0 and Team_UID = @1 and Team_Name = @2 and Name = @3 and Position = @4 and Email = @5 and Phone_Number = @6 ";
        var result = db.Query(getTeamprofile, postLeagueid, postTeamid, postTeam, postName, postPosition, postEmail, postPhone);      
       
        Json.Write(result, Response.Output);

}